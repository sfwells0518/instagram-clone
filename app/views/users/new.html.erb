<h1>Sign Up</h1>
<%= form_for @user, url: users_path, method: :post do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name, first_name: "user[first_name]" %><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name, last_name: "user[last_name]" %><br>

  <%= f.label :email_address %>
  <%= f.email_field :email_address, email_address: "user[email_address]" %><br>

  <%= f.label :date_of_birth %>
  <%= f.date_field :date_of_birth, date_of_birth: "user[date_of_birth]" %><br>

   <%= f.label :username %>
  <%= f.text_field :username, username: "user[username]" %><br>

  <%= f.label :password %>
  <%= f.password_field :password, password: "user[password]" %><br>

  <%= f.label :confirm_password, "Confirm Password" %>
  <%= f.password_field :confirm_password, confirm_password: "user[confirm_password]" %><br>

  <%= f.submit "Sign Up" %>
<% end %>